<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.qianggedemac.cem.activity.WelcomeActivity">
  <ImageView
      android:id="@+id/welcome_iv"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:background="@mipmap/sa"
      android:scaleType="fitXY"/>
    <Button
        android:id="@+id/welcome_btn"
        android:background="@mipmap/r8"
        android:layout_alignParentRight="true"
        android:layout_margin="@dimen/marginTwenty"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/jump"/>
</RelativeLayout>
